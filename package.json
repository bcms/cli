    "@types/adm-zip": "^0.4.34",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^7.3.3",
    "@types/node": "^15.6.2",
    "@types/sharp": "^0.29.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.4"
